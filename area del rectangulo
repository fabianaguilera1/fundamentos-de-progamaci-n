#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

void main ()
{
	float altura,base,area=0;
	cout<<"calcular el area del rectangulo"<<endl;
	cout<<"digite base"; cin>>base;
	cout<<"digite altura"; cin>>altura;

	
	area = (base*altura);

	cout<<"el area del rectangulo es:"<<area<<endl;


	getch();
}
