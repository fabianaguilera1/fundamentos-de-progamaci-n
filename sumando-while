#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{ int n, num, suma=0, i=1;
	cout<<"digite la cantidad de numeros que desea sumar:"; 
	cin>>n;
	 
	while( i<=n)
	{   cout<<"digite el numero;";
	    cin>>num;
		suma=suma+num; i++;
	}
	  
	 cout<<"la suma es:"<<suma<<endl;
	getch();
}
